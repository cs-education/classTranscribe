<html>
  <head>
    <script src="../javascripts/libraries/jquery-1.11.2.min.js"></script>
    <script>
      $(document).ready(function () {
        var className = "{{className}}";
        var netID = window.prompt("What's your NetID?", "");

        if (netID !== null) {
          $.post('/progress/' + className + '/' + netID, function (data) {
            document.write('An email was sent with your progress to ' + netID + '@illinois.edu');
          });
        }
      });
    </script>
  </head>
</html>
